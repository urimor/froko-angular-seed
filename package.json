{
  "name": "froko-angular-seed",
  "version": "0.1.0",
  "description": "Yet another seed project for Angular (v4) and webpack (v2)",
  "scripts": {
    "all": "npm run ci && npm start",
    "ci": "npm install --silent && npm test && npm run build",
    "syncversion": "node config/version.js",
    "clean": "rimraf aot dist",
    "compile": "ngc -p tsconfig.aot.json && rimraf trash",
    "start": "webpack-dev-server --config config/webpack.config.dev.js --port 3000 --host 0.0.0.0",
    "build": "npm run clean && npm run compile && webpack --config config/webpack.config.prod.js --profile --bail",
    "test": "karma start config/karma.conf.js",
    "test:watch": "karma start config/karma.conf.js --no-single-run --auto-watch",
    "postversion": "npm run syncversion"
  },
  "author": "Patrick Ineichen",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/froko/froko-angular-seed.git"
  },
  "dependencies": {
    "@angular/animations": "4.0.1",
    "@angular/common": "4.0.1",
    "@angular/compiler": "4.0.1",
    "@angular/compiler-cli": "4.0.1",
    "@angular/core": "4.0.1",
    "@angular/forms": "4.0.1",
    "@angular/http": "4.0.1",
    "@angular/platform-browser": "4.0.1",
    "@angular/platform-browser-dynamic": "4.0.1",
    "@angular/platform-server": "4.0.1",
    "@angular/router": "4.0.1",
    "core-js": "2.4.1",
    "reflect-metadata": "0.1.10",
    "rxjs": "5.3.0",
    "zone.js": "0.8.5"
  },
  "devDependencies": {
    "@angular/language-service": "^4.0.1",
    "@types/core-js": "0.9.36",
    "@types/jasmine": "2.5.47",
    "@types/node": "7.0.7",
    "angular2-router-loader": "0.3.5",
    "angular2-template-loader": "0.6.2",
    "awesome-typescript-loader": "3.1.2",
    "codelyzer": "3.0.0-beta.4",
    "css-loader": "0.28.0",
    "extract-text-webpack-plugin": "2.1.0",
    "file-loader": "0.11.1",
    "html-loader": "0.4.5",
    "html-webpack-plugin": "2.28.0",
    "imports-loader": "0.7.1",
    "istanbul-instrumenter-loader": "2.0.0",
    "jasmine": "2.5.3",
    "karma": "1.6.0",
    "karma-html-reporter": "0.2.7",
    "karma-jasmine": "1.1.0",
    "karma-mocha-reporter": "2.2.3",
    "karma-phantomjs-launcher": "1.0.4",
    "karma-remap-istanbul": "0.6.0",
    "karma-source-map-support": "1.2.0",
    "karma-tfs-reporter": "1.0.0",
    "karma-webpack": "2.0.3",
    "node-sass": "4.5.2",
    "null-loader": "0.1.1",
    "phantomjs-prebuilt": "2.1.14",
    "raw-loader": "0.5.1",
    "remap-istanbul": "0.9.5",
    "replace-in-file": "2.5.0",
    "resolve-url-loader": "2.0.2",
    "rimraf": "2.6.1",
    "sass-loader": "6.0.3",
    "style-loader": "0.16.1",
    "tslint": "4.5.1",
    "tslint-loader": "3.4.3",
    "typescript": "2.2.2",
    "url-loader": "0.5.8",
    "webpack": "2.3.3",
    "webpack-dev-server": "2.4.2",
    "webpack-merge": "4.1.0"
  }
}
